
@{
    Layout = null;
}

<!DOCTYPE html>

<html>

@await Html.PartialAsync("/Views/FrontPartials/HeadPartial.cshtml")

<body>

    @await Html.PartialAsync("/Views/FrontPartials/NavbarPartial.cshtml")

    <!-- Breadcrumb Section Begin -->
    <section class="breadcrumb-section set-bg" data-setbg="/GymLife/img/breadcrumb-bg.jpg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <div class="breadcrumb-text">
                        <h2>İLETİŞİM</h2>
                        <div class="bt-option">
                            <a href="/Default/Index">Ana Sayfa</a>
                            <span>İletişim</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Breadcrumb Section End -->
    <!-- Contact Section Begin -->
    <section class="contact-section spad">
        <div class="container">
            <div class="row">
                @await Component.InvokeAsync("ContactAddressPartial")


                @await Html.PartialAsync("/Views/Contact/SendMessage.cshtml")

            </div>

            @await Component.InvokeAsync("MapPartial")

        </div>
    </section>
    <!-- Contact Section End -->

    @await Component.InvokeAsync("AddressPartial")

    @await Html.PartialAsync("/Views/FrontPartials/FooterPartial.cshtml")

    @await Html.PartialAsync("/Views/FrontPartials/ScriptPartial.cshtml")

</body>

</html>


